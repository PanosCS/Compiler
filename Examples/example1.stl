//An example
program example1
    declare d,i,g,f;

    function two (in g)
        function three (in g, inout x, inout m)
            declare k, j;

            k:= g;
            j:=g;

            dowhile
                if (k>i) then
                    k:=k-1;
                endif;

                j:=j*k;
                k:=k+g
            enddowhile (k<10);

            m:=j;
            return(m+1);

        endfunction

        i:= three (in i+2, inout d, inout f);
        return(0);
    endfunction


    function one (in g)
        g:=two(in g);
        return(0);
    endfunction

    i:=5;
    g:=1;
    g:= one(in g)
endprogram